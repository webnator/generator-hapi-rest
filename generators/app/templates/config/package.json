{
  "name": "<%= appUppercaseName %>",
  "version": "0.0.1",
  "description": "Generic backend Hapi App generated with the hapi-rest generator",
  "main": "server",
  "dependencies": {
    "hapi": "11.x.x",
    "hapi-auth-jwt2": "5.2.1",
    "hapi-mongodb": "4.1.0",
    "inert": "3.2.0",
    "joi": "7.0.1",
    "lodash": "4.0.0",
    "q": "1.4.1",
    "uuid": "2.0.1",
    "vision": "4.0.1",
    "winston": "2.2.0",
    "request": "2.69.0"
  },
  "devDependencies": {
    "jshint": "2.x",
    "gulp-jshint": "2.0.0",
    "gulp-nodemon": "2.0.4",
    "gulp-shell": "^0.5.2"
    <% if (usesRAML) { %>, "gulp-raml2html": "^0.2.0"<% } %>
    <% if (usesTests) { %>, "gulp-istanbul": "^0.10.4",
    "gulp-jasmine": "^2.3.0",
    "gulp-process-env": "0.0.1",
    "isparta": "^4.0.0",
    "jasmine-node": "^1.14.5",
    "rewire": "^2.5.1"<% } %>
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "start": "node server"<% if (usesTests) { %>,
    "test": "jasmine-node tests --config <%= appPrefix %>_NODE_ENV test --color"<% } %>
  },
  "private": true
}
